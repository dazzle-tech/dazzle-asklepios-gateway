<?xml version="1.1" encoding="utf-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
      http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

  <changeSet id="BM000014" author="bushra.mashaiekh">
    <createTable tableName="diagnostic_test_pathology">
      <column name="id" type="bigint" autoIncrement="true" startWith="56765">
      </column>
      <column name="test_id" type="text"/>
      <column name="international_coding_type" type="text"/>
      <column name="child_code" type="text"/>
      <column name="pathology_category" type="text"/>
      <column name="specimen_type" type="text"/>
      <column name="analysis_procedure_lkey" type="text"/>
      <column name="turnaround_time" type="text"/>
      <column name="time_unit" type="text"/>
      <column name="created_by" type="varchar(50)">
        <constraints nullable="false"/>
      </column>
      <column name="created_date" type="timestamp"/>
      <column name="reset_date" type="timestamp">
        <constraints nullable="true"/>
      </column>
      <column name="last_modified_by" type="varchar(50)"/>
      <column name="last_modified_date" type="timestamp"/>
      <column name="test_description" type="text"/>
      <column name="sample_handling" type="text"/>
      <column name="medical_indications" type="text"/>
      <column name="critical_values" type="text"/>
      <column name="preparation_requirements" type="text"/>
      <column name="associated_risks" type="text"/>
      <column name="path_catalog_id" type="text"/>
    </createTable>
    <dropDefaultValue tableName="diagnostic_test_pathology" columnName="created_date" columnDataType="${datetimeType}"/>

  </changeSet>

</databaseChangeLog>
