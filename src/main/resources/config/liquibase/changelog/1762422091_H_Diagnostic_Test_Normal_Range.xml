<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
    http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

  <!-- ============================================================ -->
  <!-- Table: diagnostic_test_normal_range                          -->
  <!-- ============================================================ -->
  <changeSet id="H00000000000030" author="hanan253yahya">

    <createTable tableName="diagnostic_test_normal_range">
      <column name="id" type="bigint" autoIncrement="true">
        <constraints nullable="false" primaryKey="true"/>
      </column>

      <column name="test_id" type="bigint">
        <constraints nullable="false"/>
      </column>

      <column name="gender" type="varchar(100)"/>
      <column name="age_from" type="numeric"/>
      <column name="age_from_unit" type="varchar(100)"/>
      <column name="age_to" type="numeric"/>
      <column name="age_to_unit" type="varchar(100)"/>
      <column name="condition" type="varchar(100)"/>

      <column name="result_type" type="varchar(100)">
        <constraints nullable="false"/>
      </column>

      <column name="result_text" type="varchar(500)"/>
      <column name="result_lov" type="varchar(200)"/>
      <column name="normal_range_type" type="varchar(100)"/>
      <column name="range_from" type="numeric"/>
      <column name="range_to" type="numeric"/>

      <column name="critical_value" type="boolean" defaultValue="false"/>
      <column name="critical_value_less_than" type="numeric"/>
      <column name="critical_value_more_than" type="numeric"/>

      <column name="profile_test_id" type="bigint"/>
      <column name="is_profile" type="boolean" defaultValue="false"/>
    </createTable>


    <addForeignKeyConstraint
      baseTableName="diagnostic_test_normal_range"
      baseColumnNames="test_id"
      referencedTableName="diagnostic_test"
      referencedColumnNames="id"
      constraintName="fk_test_id_normal_range"/>
    <addForeignKeyConstraint
      baseTableName="diagnostic_test_normal_range"
      baseColumnNames="profile_test_id"
      referencedTableName="diagnostic_test_profile"
      referencedColumnNames="id"
      constraintName="fk_profile_test_id_normal_range"/>
  </changeSet>


  <!-- ============================================================ -->
  <!-- Table: diagnostic_test_normal_range_lov                      -->
  <!-- ============================================================ -->
  <changeSet id="H00000000000031" author="hanan253yahya">
    <createTable tableName="diagnostic_test_normal_range_lov">
      <column name="id" type="bigint" autoIncrement="true">
        <constraints nullable="false" primaryKey="true"/>
      </column>

      <column name="normal_range_id" type="bigint">
        <constraints nullable="false"/>
      </column>

      <column name="lov" type="varchar(100)">
        <constraints nullable="false"/>
      </column>


    </createTable>

    <addForeignKeyConstraint
      baseTableName="diagnostic_test_normal_range_lov"
      baseColumnNames="normal_range_id"
      referencedTableName="diagnostic_test_normal_range"
      referencedColumnNames="id"
      constraintName="fk_normal_range_lov_id"/>
  </changeSet>


</databaseChangeLog>
